RewriteEngine on

RewriteRule ^(flv|flvdl|image|album|folder|meta|photo|comments|slide\-show|private)/(.*)$ /index.php?/$1/$2

RewriteRule ^(select|unselect|selectiong|deleteselection|downloadselection)/(.*)$ /index.php?/$1/$2

RewriteRule ^(.*)/_javascript/(.*)$ /_javascript/$2 [L]
RewriteRule ^(.*)/templates/(.*)$ /templates/$2 [L]


