RewriteEngine on


RewriteRule ^image/(.*)$ /img.php?$1 [L]
RewriteRule ^(.+)\.html$ /index.php?/$1\.html

RewriteCond %{REQUEST_URI} !^/slideshow.php
RewriteRule ^(.*)slideshow.php(.*)$ /slideshow.php


RewriteRule ^(.*)/_javascript/(.*)$ /_javascript/$2 [L]
RewriteRule ^(.*)/templates/(.*)$ /templates/$2 [L]
RewriteRule ^(.*)/_images/(.*)$ /_images/$2 [L]
